algoritmo "CarroAlugado"
var
   Km, R: Real
   Dias: Inteiro
inicio
      Escreva("Quantidade de KM's rodados? KM")
      Leia(Km)
      Escreva("Quantos Dias o carro foi alugado? ")
      Leia(Dias)
      R <- (0.20 * KM) + (90 * Dias)
      Escreva("O total a ser pago pelo aluguel do carro é de",R,"R$")
      

fimalgoritmo