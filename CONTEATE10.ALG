algoritmo "conteAte10"
var
   valor,salto, contador: inteiro
inicio
      Contador <- 0
      Escreva("Ate onde voce quer contar? ")
      Leia(valor)
      Escreva("valor dos saltos? ")
      Leia(salto)
      enquanto (Contador <= valor) faca
               Escreval(contador)
               contador <- contador + salto
      Fimenquanto
      Escreva("Terminei de contar")
fimalgoritmo